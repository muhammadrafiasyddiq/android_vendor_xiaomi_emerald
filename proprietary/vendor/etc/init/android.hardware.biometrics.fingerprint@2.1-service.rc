on post-fs-data
   mkdir /mnt/vendor/persist/fpc
   chown system system /mnt/vendor/persist/fpc
   chmod 0700 /mnt/vendor/persist/fpc

   mkdir /data/vendor/fpdump
   chown system system /data/vendor/fpdump
   chmod 0700 /data/vendor/fpdump
service vendor.fps_hal /vendor/bin/hw/android.hardware.biometrics.fingerprint@2.1-service
    # "class hal" causes a race condition on some devices due to files created
    # in /data. As a workaround, postpone startup until later in boot once
    # /data is mounted.
    class late_start
    user system
    group system input uhid
#N6 add for HQ-304357 by sunyuting at 20230817 start
    #writepid /dev/cpuset/system-background/tasks
#N6 add for HQ-304357 by sunyuting at 20230817 end
#N6 code for HQ-304309 by zhujingyi at 2023/08/17 start
on property:vendor.fps_hal.restartimes=max
    stop vendor.fps_hal
#N6 code for HQ-304309 by zhujingyi at 2023/08/17 end
